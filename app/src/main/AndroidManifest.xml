<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.vishal.GecaAlumni">

    <uses-sdk tools:ignore="GradleOverrides" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/geca"
        android:label="GecaAlumni"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.Registration" />
        <activity
            android:name=".Activity.Navigation"
            android:label="ALUMNI"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activity.fgpassword" />
        <activity android:name=".Activity.Profile" />
        <activity android:name=".Activity.Findothers" />
        <activity android:name=".Activity.Events" />
        <activity android:name=".Activity.Walloffame" />
        <activity android:name=".Activity.EditProfile" />
        <activity android:name=".Activity.News" />
        <activity android:name=".Activity.Contact_Us" />
        <activity android:name=".Activity.AboutUs" />
        <activity android:name=".Activity.MemberPro" />
        <activity android:name=".Activity.ChatWindow" />
        <activity android:name=".Activity.JobAvailable" />
        <activity android:name=".Activity.JobPosting" />
        <activity android:name=".Activity.JobDescription" />
        <activity android:name=".Activity.AlumniCategory" />
        <activity android:name=".Activity.Donates" />
        <activity android:name=".Activity.ChatList" />
        <activity android:name=".Activity.FirstSendMsg" />
        <activity
            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait" />
        <activity android:name=".Activity.checksum"></activity>
    </application>

</manifest>